@page
@model NutriTic.App.Frontend.Pages.EliminarEmpleadoModel
@{
}
<h1>Eliminar Paciente</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Empleado.IdEmpleado"></label>
        <input asp-for="Empleado.IdEmpleado" class="form-control" readonly>
    </div>

    <div class="form-group">
        <label asp-for="Empleado.PrimerNombre"></label>
        <input asp-for="Empleado.PrimerNombre" class="form-control" readonly>
    </div>

    <button type="submit" class="btn btn-primary">Eliminar</button>
</form>
